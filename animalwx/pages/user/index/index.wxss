page{
    background:#f8f5f8 ;
}
.form-item{
    width: 100%;
    height: 300rpx;
    display: flex;
    flex-direction:column;
    position: relative;
}
.form-item-bgimg{
    width: 100%;
}
.form-item-infor{
    width: 100%;
    height: 200rpx;
    position: absolute;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}
.form-item-img{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50px;
    margin: auto;
}
.form-item-login{
    margin: auto;
    font-size: 35rpx;
    font-family:楷体;
    background: inherit;
    display: flex;
    flex-direction: row;
}
.form-self{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-evenly;
    position: absolute;
    margin-top: 210rpx;
}
.form-selfstyle{
    display: flex;
    flex-direction: column;
    font-size: 35rpx;
    font-family:楷体;
}
.form-selfstyle-text{
    margin: 0 auto;
}
.form-list{
    width: 100%;
    height: 200rpx;
    margin-top: 25rpx;
    
}
.form-listext{
    width: 100%;
    height: 100rpx;
    margin-top: 5rpx;
    display: flex;
    flex-direction: row;
    background: white;
    font-family: 楷体;
    font-size: 35rpx;
}
.list{
    width: 25%;
    margin: auto;
    margin-left: 50rpx;
}